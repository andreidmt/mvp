{
  "$id": "carts",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Cart": {
      "additionalProperties": false,
      "properties": {
        "createdAt": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "items": {
          "items": {
            "$ref": "#/definitions/CartItem"
          },
          "type": "array"
        },
        "updatedAt": {
          "type": "string"
        },
        "userId": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "userId",
        "items",
        "createdAt",
        "updatedAt"
      ],
      "type": "object"
    },
    "CartItem": {
      "additionalProperties": false,
      "properties": {
        "createdAt": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "productId": {
          "type": "string"
        },
        "quantity": {
          "type": "number"
        },
        "updatedAt": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "quantity",
        "productId",
        "createdAt",
        "updatedAt"
      ],
      "type": "object"
    }
  }
}